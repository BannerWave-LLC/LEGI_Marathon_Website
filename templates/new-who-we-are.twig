{% extends "_layouts/default.twig" %}

{% block bodyTag %}
  {{ parent()|attr({
    class: 'bg-gray-200'
  }) }}
{% endblock %}

{% block content %}
  {% include "_partials/header-landing.twig" %}

  <div class="py-26p pb-10 lg:py-50p">
    <div class="container">
      <div>
        {% include "_partials/nav-team-filter.twig" %}

        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-x-2.5 gap-y-5 max-w-[1154px] mx-auto mb-31p lg:mb-99p lg:pl-1 lg:gap-x-5 lg:gap-y-10">
          {% for i in 1..35 %}
            {% include "_partials/tile-bio.twig" %}
          {% endfor %}
        </div>

        {% include "_partials/nav-pager.twig" %}
      </div>
    </div>
  </div>

  {% include "_partials/cta-about-us.twig" %}

  {% include "_partials/nav-footer.twig" %}
{% endblock %}
